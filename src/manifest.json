{
  "name": "Cachable UI",
  "description": "Gestion de cache avanc√©e",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "res/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "page/script.js"
      ],
      "css": ["page/new_style.css"],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}